<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="dk.itu.ssase.hb.beans">
  <class name="Relationship" table="relationship" rowid="id">
                <id name="id">
                        <generator class="native"/>
                </id>
      <many-to-one name="student1" class="Student" column="student1"/>
      <many-to-one name="student2" class="Student" column="student2"/>
    <property name="type"/>
  </class>
</hibernate-mapping>
