<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="dk.itu.ssase.hb.beans.model">
  <class name="Relationship" rowid="id" table="relationship">
    <id name="id">
      <generator class="native"/>
    </id>
    <many-to-one class="Student" column="student1" name="student1"/>
    <many-to-one class="Student" column="student2" name="student2"/>
    <property name="type"/>
  </class>
</hibernate-mapping>
